<!DOCTYPE html>
<html>
<head prefix="og: http://ogp.me/ns#">
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />

    <!--Meta Data for Open Graph-->
    <meta property="og:title" content="TestLegends - School Reimagined." />
    <meta property="og:type" content="website" />
	<meta property="og:image" content="http://m.testlegends.com/images/normal.png" />
	<meta property="og:url" content="http://testlegends.com/" />
	<meta property="og:site_name" content="TestLegends" />
	<meta property="og:description" content="School reimagined!" />

    <title>TestLegends</title>

    <link rel="stylesheet" href="styles/home.min.css" />
</head>

<body onload="preload()">
    <div id="fullpage">
        <div id="box1" class="section">
            <landing></landing>
        </div>

        <div id="box2" class="section">
            <div id='canvas_container' style="position: absolute; z-index: 100; width: 100%">
                <canvas id="EpicGame" width="2272" height="1280" style="width: 100%"></canvas>
            </div>
            <demo></demo>
        </div>

        <div id="box3" class="section">
            <customize></customize>
        </div>

        <div id="box4" class="section">
            <track></track>
        </div>

        <div id="box5" class="section">
            <publish></publish>
        </div>

        <div id="box6" class="section">
            <signup></signup>
        </div>
    </div>

    <script src="js/vendor/requirejs/require.js" data-main="js/angular/home.min"></script>
    <script src="js/home.js"></script>

    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
    <script src="http://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.6.0/underscore-min.js"></script>
    <script src="http://cdnjs.cloudflare.com/ajax/libs/q.js/0.9.2/q.min.js"></script>
    <script src="http://code.createjs.com/easeljs-0.7.1.min.js"></script>
    <script src="http://code.createjs.com/tweenjs-0.5.1.min.js"></script>
    <script src="http://code.createjs.com/soundjs-0.5.2.min.js"></script>
    <script src="http://code.createjs.com/preloadjs-0.4.1.min.js"></script>
    <script src="/js/game/ContentManager.js"></script>
    <script src="/js/game/box_ui.js"></script>
    <script src="/js/game/hero.js"></script>
    <script src="/js/game/monster.js"></script>
    <script src="/js/game/timer.js"></script>
    <script src="/js/game/Questions.js"></script>
    <script src="/js/game/init.js"></script>
    <script>
    global_width = 2272;
    global_height = 1280;

    // createjs, show dom before it is animated
    show_dom = function (id_name) {
        $('#'+id_name).show();
    }

    $(window).on("resize", function(){
        var ratio = 640/1136;
        var update_w = window.innerWidth;
        $('#canvas_container').css('width', update_w);
        $('#canvas_container').css('height', update_w*ratio);
        style_width = update_w;
        style_height = update_w * ratio;
        // preload();
    });
    </script>

    <%- partial('../partials/google_analytics') %>
    <%- partial('../partials/uservoice') %>
</body>

</html>
